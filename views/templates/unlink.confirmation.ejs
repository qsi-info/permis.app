<div class="row">
	<div class="col-md-12">


		<h3>Confirmez la dissociation</h3>


		<div class="row">
			<div class="col-md-8">

				<div class="panel panel-warning">
				  <div class="panel-heading">
				    <h4 class="panel-title">
				    	<%= __('Équipement associé') %>
				    </h4>
				  </div>
				  <div class="panel-body">
				  	<h4>{{link.Equipement_Code}}</h4>
						<p>{{link.Equipement_Description}}</p>
						<p>{{link.Niveau_Description}}</p>
				  </div>			
				</div>			


				<div class="panel panel-danger">
				  <div class="panel-heading">
				    <h4 class="panel-title">
				    	<%= __('Fiche à dissocier') %>
				    </h4>
				  </div>
				  <div class="panel-body">
				  	<p>{{link.Nom_Fichier}}</p>
				  </div>			
				</div>			


				<div class="pull-right">
					<button ng-click="confirm()" class="btn btn-success">
						<%= __('Confirmez') %>
					</button>

					<a href="#/home" class="btn btn-default">
						<%= __('Annuler') %>
					</a>

				</div>

			</div>

		</div>



	</div>	
</div>


